<Window x:Class="VengaComoNo.eliminaProducto"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VengaComoNo"
        mc:Ignorable="d"
        Title="eliminaProducto" Height="300" Width="300">
    <Grid>
        <Label x:Name="lbId" Content="Id Producto:" HorizontalAlignment="Left" Margin="40,57,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtIdBaja" HorizontalAlignment="Left" Height="23" Margin="162,57,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btBaja" Content="Baja" HorizontalAlignment="Left" Margin="193,109,0,0" VerticalAlignment="Top" Width="75" Click="btBaja_Click" />
        <Button x:Name="btRegresar" Content="Regresar" HorizontalAlignment="Left" Margin="193,151,0,0" VerticalAlignment="Top" Width="75" Click="btRegresar_Click" />
    </Grid>
</Window>
