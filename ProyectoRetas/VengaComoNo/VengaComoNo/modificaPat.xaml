<Window x:Class="VengaComoNo.modificaPat"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VengaComoNo"
        mc:Ignorable="d"
        Title="modificaPat" Height="300" Width="300">
    <Grid>
        <Label x:Name="label" Content="Nuevo correo" HorizontalAlignment="Left" Height="27" Margin="27,72,0,0" VerticalAlignment="Top" Width="103"/>
        <TextBox x:Name="txtMail" HorizontalAlignment="Left" Height="27" Margin="152,72,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="116"/>
        <Button x:Name="btModifica" Content="Modificar&#xD;&#xA;" HorizontalAlignment="Left" Height="31" Margin="41,137,0,0" VerticalAlignment="Top" Width="89" Click="btModifica_Click"/>
        <Button x:Name="btRegresa" Content="Regresar" HorizontalAlignment="Left" Height="31" Margin="179,137,0,0" VerticalAlignment="Top" Width="89" Click="btRegresa_Click"/>
        <Label x:Name="label_Copy" Content="Id a modificar " HorizontalAlignment="Left" Height="27" Margin="27,25,0,0" VerticalAlignment="Top" Width="103"/>
        <TextBox x:Name="txtId" HorizontalAlignment="Left" Height="27" Margin="152,25,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="116"/>

    </Grid>
</Window>
